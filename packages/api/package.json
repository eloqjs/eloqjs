{
  "name": "@eloqjs/api",
  "version": "0.0.1-dev",
  "keywords": [
    "eloqjs",
    "eloquent",
    "orm"
  ],
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/eloqjs/eloqjs.git",
    "directory": "packages/api"
  },
  "license": "MIT",
  "author": "Jo√£o Pedro Antunes Silva",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./httpclient/axios": {
      "types": "./dist/httpclient/axios.d.ts",
      "require": "./dist/httpclient/axios.cjs",
      "import": "./dist/httpclient/axios.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "coverage": "jest --collect-coverage",
    "lint": "eslint --ext .ts,.js .",
    "lint:fix": "yarn lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "stub": "unbuild --stub"
  },
  "peerDependencies": {
    "axios": "*"
  },
  "peerDependenciesMeta": {
    "axios": {
      "optional": true
    }
  },
  "dependencies": {
    "@eloqjs/core": "0.0.1-dev",
    "defu": "^5.0.1"
  },
  "devDependencies": {
    "axios": "^0.27.2",
    "axios-mock-adapter": "^1.20.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "f8c3e937e1f20e6b6200fcb20b87d0b3e0afef36"
